// https://mediasession.spec.whatwg.org/
// https://github.com/whatwg/mediasession/pull/129 (spec proposal for artwork)

[
    ConstructorCallWith=ExecutionContext,
    Constructor(MediaArtworkInit artwork),
    RuntimeEnabled=MediaSession,
] interface MediaArtwork {
  readonly attribute USVString src;
  readonly attribute DOMString sizes;
  readonly attribute DOMString type;
};
