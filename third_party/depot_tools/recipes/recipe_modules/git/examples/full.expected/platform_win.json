[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]\\bootstrap\\win\\git_bootstrap.py",
      "--verbose"
    ],
    "cwd": "RECIPE_PACKAGE_REPO[depot_tools]",
    "infra_step": true,
    "name": "ensure git tooling on windows",
    "timeout": 300
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]\\resources\\git_setup.py",
      "--path",
      "[START_DIR]\\src",
      "--url",
      "https://chromium.googlesource.com/chromium/src.git",
      "--git_cmd_path",
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat"
    ],
    "name": "git setup"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "retry",
      "fetch",
      "origin",
      "master",
      "--recurse-submodules"
    ],
    "cwd": "[START_DIR]\\src",
    "env": {
      "PATH": "RECIPE_PACKAGE_REPO[depot_tools];<PATH>"
    },
    "infra_step": true,
    "name": "git fetch"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "checkout",
      "-f",
      "FETCH_HEAD"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git checkout"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "rev-parse",
      "HEAD"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "read revision",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>checked out 'deadbeef'<br/>@@@"
    ]
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "clean",
      "-f",
      "-d",
      "-x"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git clean"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "submodule",
      "sync"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "submodule sync"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "submodule",
      "update",
      "--init",
      "--recursive"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "submodule update"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "-c",
      "foo=bar",
      "count-objects",
      "-v"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "count-objects",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "config",
      "--get",
      "remote.origin.url"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git config remote.origin.url",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_TEXT@foo@@@"
    ]
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "show",
      "HEAD",
      "--format=%at",
      "-s"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git show",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "fetch",
      "origin",
      "--tags"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git fetch tags"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "status"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git status"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "status"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git status can_fail_build"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "status"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git status cannot_fail_build"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "new-branch",
      "refactor"
    ],
    "cwd": "[START_DIR]\\src",
    "env": {
      "PATH": "RECIPE_PACKAGE_REPO[depot_tools];<PATH>"
    },
    "infra_step": true,
    "name": "git new-branch refactor"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "new-branch",
      "feature",
      "--upstream",
      "refactor"
    ],
    "cwd": "[START_DIR]\\src",
    "env": {
      "PATH": "RECIPE_PACKAGE_REPO[depot_tools];<PATH>"
    },
    "infra_step": true,
    "name": "git new-branch feature"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "rebase",
      "origin/master"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "my repo rebase"
  },
  {
    "cmd": [
      "RECIPE_PACKAGE_REPO[depot_tools]\\git.bat",
      "bundle",
      "create",
      "[START_DIR]\\all.bundle",
      "--all"
    ],
    "cwd": "[START_DIR]\\src",
    "infra_step": true,
    "name": "git bundle"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]