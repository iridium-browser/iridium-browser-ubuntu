{
  "manifest_version": 1,
  "name": "exe:chrome",
  "display_name": "Chrome",
  "capabilities": {
    "required": {
      "*": { "classes": [ "app" ] },
      "mojo:ash": {
        "interfaces": [
          "ash::mojom::WallpaperController",
          "mash::shelf::mojom::ShelfController"
        ]
      }
    },
    "provided": {
      "mash:launchable": [ "mash::mojom::Launchable" ],
      "app_list:presenter": [ "app_list::mojom::AppListPresenter" ]
    }
  }
}
