    "contents": {
      "type_name": "FieldTrialTestingGroup",
      "type": "struct",
      "fields": [
        {"field": "study", "type": "string"},
        {"field": "group_name", "type": "string"},
        {
          "field": "params",
          "type": "array",
          "contents": {
            "type_name": "FieldTrialGroupParams",
            "type": "struct",
            "fields": [
              {"field": "key", "type": "string"},
              {"field": "value", "type": "string"}
