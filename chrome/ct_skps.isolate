{
  'conditions': [
    ['OS=="linux"', {
      'variables': {
        'files': [
          '../../skia/resources/',
          '../../skia/out/<(CONFIGURATION)/<(TOOL_NAME)',
          '../content/test/ct/run_ct_skps.py',
          '../../skps/slave<(SLAVE_NUM)/',
        ],
        'command': [
          'python',
          '../content/test/ct/run_ct_skps.py',
          '--slave_num', '<(SLAVE_NUM)',
          '--tool', '<(TOOL_NAME)',
          '--git_hash', '<(GIT_HASH)',
          '--isolated_outdir', '${ISOLATED_OUTDIR}',
          '--configuration', '<(CONFIGURATION)',
          '--builder', '<(BUILDER)',
        ],
      },
    }],
  ]
}
