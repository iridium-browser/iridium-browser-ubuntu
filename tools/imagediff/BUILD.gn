executable("imagediff") {
  output_name = "image_diff"  # Different than dir nam for historical reasons.
  sources = [
    "image_diff.cc",
    "image_diff_png.cc",
    "image_diff_png.h",
  ]

  configs += [ "//build/config/compiler:wexit_time_destructors" ]

  deps = [
    "//base",
    "//third_party/libpng",
    "//third_party/zlib",
  ]
}
