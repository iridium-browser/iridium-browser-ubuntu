{
  "name": "test",
  "description": "App with file_handlers manifest.",
  "version": "1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "file_handlers": {
    "text": {
      "types": [
        "text/*"
      ]
    },
    "verb": {
      "extensions": ["*"],
      "verb": "invalid_verb"
    }
  }
}
