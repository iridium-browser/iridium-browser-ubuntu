# Targets may depend on this to bring in every CMA component.
group("cma") {
  public_deps = [
    "//chromecast/media/cma/backend",
    "//chromecast/media/cma/base",
    "//chromecast/media/cma/ipc",
    "//chromecast/media/cma/ipc_streamer",
    "//chromecast/media/cma/pipeline",
  ]
}
