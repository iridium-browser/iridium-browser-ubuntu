    {
      "id": "open-hosted-gdoc",
      "default_title": "__MSG_HOSTED__",
      "default_icon": "common/images/file_types/200/generic.png",
      "file_filters": [
        "filesystem:*.gdoc"
      ]
    },
    {
      "id": "open-hosted-gsheet",
      "default_title": "__MSG_HOSTED__",
      "default_icon": "common/images/file_types/200/generic.png",
      "file_filters": [
        "filesystem:*.gsheet"
