{
  "manifest_version":  1,
  "name": "mojo:task_viewer",
  "display_name": "Task Viewer",
  "capabilities": {
    "provided": { "mash:launchable": ["mash::mojom::Launchable"] },
    "required": {
      "*": { "classes": [ "app" ] },
      "mojo:catalog": {
        "interfaces": [ "catalog::mojom::Catalog" ]
      },
      "mojo:shell": { "interfaces": [ "shell::mojom::ServiceManager" ] }
    }
  }
}
